#include "libc.jit"

struct pair { int a, int b };

struct nested { pair a, pair b };

struct pairp { int *a, int *b };

struct nestedp { pairp* a, pairp *b };

fun test(int a) int { return a * 3; }

fun main() int {
        let int a = 1;
        let int b = 2;
        let int c = 3;
        let int d = 4;

        let pairp p1 = {&a, &b};
        let pairp p2 = {&c, &d};

        let nestedp np = {&p1, &p2};
        let nestedp* npp = &np;

        return *npp->b->b;
}
