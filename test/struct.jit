struct pair { int a, int b };

struct nested { pair a, pair b };

fun test(int a) int {
        return a * 2;
}

fun main(pair* z) void {
        let pair p = {1, test(5)};
        let pair p2 = {1, 2};
        let pair* p3 = &p2;

        let nested n = { {1, 2}, {3, 4}};
        n.a = {5, 6};
        n.a.a = 7;
}
