#include "libc.jit"

struct pair { int a, int b };

fun mult(pair* p) int {
        let int ret = 0;
        for (let int i = 1; i != p->b + 1; i = i + 1)
                ret = ret + p->a;
        rof
        return ret;
}

fun print_pair(pair* p) void {
        printf("{%lld, %lld}", p->a, p->b);
}

fun fact(int<4> n) int {
        let int ret = 1;
        for (let int i = 1; i != n + 1; i = i + 1)
                let pair p = {ret, i};
                print_pair(&p);
                ret = mult(&p);
                printf(" => %lld\n", ret);
        rof
        printf("fact => %lld\n", ret);
        return ret;
}

fun main(int<4> a) int {
        for (let int<4> i = 0; i != a; i = i + 1)
                printf("%lld\n", fact(i));
        rof

        return a;
}
